    "compilerOptions": {
      "target": "ESNext",
      "lib": ["ESNext", "DOM"],
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "moduleResolution": "Node",
      "strict": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "skipLibCheck": true,
      "declaration": true,
      "outDir": "dist"
    },
    "include": [
      "src"
    ],
  }
  